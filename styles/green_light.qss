    min-width: 36px;
    min-height: 32px;
    padding: 0;
    background-color: #E8F5E9;
    border: none;
    border-radius: 6px;
    color: #006B1A;
    font-size: 14px;
}

QPushButton#searchButton:hover {
    background-color: #C8E6C9;
    color: #008E40;
}

QPushButton#searchButton:pressed {
    background-color: #A5D6A7;
    color: #006B1A;
}

QPushButton#searchButton:disabled {

    color: #006B1A;
}

/* –î–∏–∞–ª–æ–≥ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è/–æ—Ç–∫—Ä—ã—Ç–∏—è —Ñ–∞–π–ª–∞ (Qt, –Ω–µ –Ω–∞—Ç–∏–≤–Ω—ã–π) ‚Äî –≤ —Ç–æ–Ω –ø—Ä–∏–ª–æ–∂–µ–Ω–∏—è */
QFileDialog {
    background-color: #F5FBF6;
}
QFileDialog QWidget {
    font-size: 12px;
}
QFileDialog QDialogButtonBox {
    margin-top: 12px;
    padding-top: 12px;
    border-top: 1px solid #E8F0E9;
}
QFileDialog QDialogButtonBox QPushButton {
    min-height: 32px;
    min-width: 72px;
}

/* –ü—Ä–µ–≤—å—é –ø–∏–∫—Ç–æ–≥—Ä–∞–º–º—ã –≤ —Ñ–æ—Ä–º–µ —Ç–æ–≤–∞—Ä–∞/—É—Å–ª—É–≥–∏ */
QLabel#iconPreviewLabel {
    background-color: #FAFAFA;
    border: 1px solid #E8F0E9;
    border-radius: 6px;
    color: #5A6B5C;
}

/* –ö–Ω–æ–ø–∫–∏ ¬´–û–±–∑–æ—Ä‚Ä¶¬ª –≤ —Ñ–æ—Ä–º–∞—Ö ‚Äî –∫–∞–∫ –æ—Å–Ω–æ–≤–Ω—ã–µ, –Ω–æ –º–æ–∂–Ω–æ —á—É—Ç—å —Å–ø–æ–∫–æ–π–Ω–µ–µ */
QPushButton#browseImageButton, QPushButton#browseLogoButton,
QPushButton#logPathBrowseButton, QPushButton#brandingLogoBrowseButton {
    min-width: 72px;
}

/* ---- –ü—Ä–æ–≥—Ä–µ—Å—Å ---- */
QProgressBar {
    border: none;
    border-radius: 4px;
    text-align: center;
    background-color: #E8F0E9;
}

QProgressBar::chunk {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 #00A848,
        stop:1 #008E40);
    border-radius: 4px;
}
